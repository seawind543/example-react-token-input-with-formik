{
  "name": "example-react-token-input-with-formik",
  "version": "1.0.0",
  "description": "An Example about use React TokenInput (react-customize-token-input) with Formik",
  "main": "lib/index.js",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "prepublish": "yarn run eslint && yarn run clean && yarn run build && yarn run build:examples",
    "build": "webpack",
    "build:examples": "cd examples; webpack",
    "clean": "rm -f {lib,dist}/*",
    "eslint": "eslint --max-warnings 0 --ext .jsx,.js,.ts,.tsx examples src test",
    "start": "cd examples; webpack && webpack serve --hot --inline --host 0.0.0.0 --port 8000 --content-base ../docs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix --max-warnings 0"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:seawind543/example-react-token-input-with-formik.git"
  },
  "author": "Mark Lin <marklin1106@gmail.com>",
  "homepage": "https://github.com/seawind543/example-react-token-input-with-formik",
  "license": "MIT",
  "keywords": [
    "react",
    "token",
    "tag",
    "react-customize-token-input",
    "formik"
  ],
  "dependencies": {
    "formik": "^2.2.9",
    "prop-types": "^15.8.1",
    "react-customize-token-input": "^1.4.2",
    "react": "^16.10.2",
    "react-dev-utils": "^11.0.4",
    "react-dom": "^16.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/eslint-plugin": "^7.16.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-react-qa-classes": "^1.6.0",
    "classnames": "^2.3.1",
    "css-loader": "^6.6.0",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "email-validator": "^2.0.4",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-webpack-plugin": "^3.1.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "mini-css-extract-plugin": "^2.5.3",
    "nib": "^1.1.2",
    "prettier": "^2.5.1",
    "style-loader": "^3.3.1",
    "stylelint": "^14.5.3",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-webpack-plugin": "^3.1.1",
    "stylus": "^0.56.0",
    "stylus-loader": "^6.2.0",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^3.11.3"
  }
}
