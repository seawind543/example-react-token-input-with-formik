{
  "name": "example-react-token-input-with-formik",
  "version": "2.2.0",
  "description": "An Example about use React TokenInput (react-customize-token-input) with Formik",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "prepublish": "yarn run lint && yarn run build",
    "build": "yarn run build:clean && yarn run build:scss-types && yarn run build:webpack && yarn run build:examples",
    "build:clean": "rm -rf lib/*",
    "build:scss-types": "typed-scss-modules '{src,examples}/**/*.scss' --nameFormat none --exportType default",
    "build:webpack": "webpack",
    "build:examples": "cd examples; webpack",
    "lint": "yarn run lint:eslint && yarn run lint:style",
    "lint:eslint": "eslint --max-warnings 0 --ext .jsx,.js,.tsx,.ts src examples",
    "lint:style": "stylelint --custom-syntax postcss-scss '**/*.scss'",
    "start": "cd examples; webpack && webpack serve"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix --max-warnings 0"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:seawind543/example-react-token-input-with-formik.git"
  },
  "author": "Mark Lin <marklin1106@gmail.com>",
  "homepage": "https://github.com/seawind543/example-react-token-input-with-formik",
  "license": "MIT",
  "keywords": [
    "react",
    "token",
    "tag",
    "react-customize-token-input",
    "formik"
  ],
  "dependencies": {
    "classnames": "^2.3.1",
    "formik": "^2.2.9",
    "react": "^17.0.2",
    "react-customize-token-input": "2.2.0",
    "react-dev-utils": "^12.0.1",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/eslint-plugin": "^7.17.7",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/react": "^17.0.47",
    "@types/react-dom": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-transform-react-qa-classes": "^1.6.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "email-validator": "^2.0.4",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.1.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^3.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "node-sass": "^7.0.1",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.0",
    "postcss-preset-env": "^7.7.2",
    "prettier": "^2.7.1",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.2.0",
    "stylelint-selector-bem-pattern": "^2.1.1",
    "stylelint-webpack-plugin": "^3.3.0",
    "typed-scss-modules": "^6.5.0",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  }
}
